!!! 5
%html(lang="en")
  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=Edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}
    %title= "Event Roster Report"


    = stylesheet_link_tag "//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css", media: "all"
    = stylesheet_link_tag "//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.2.0/css/font-awesome.min.css", media: "all"
    = #stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.2.6/fullcalendar.print.css", media: "all"

    = stylesheet_link_tag "print", media: "all"

    = javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"
    = javascript_include_tag "//maps.google.com/maps/api/js?sensor=true"
    = javascript_include_tag "//cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.12/gmaps.min.js"
    = javascript_include_tag "printing/print.js"

  %body
    .container-fluid
      .row
        .col-md-12
          = yield
